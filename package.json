{
  "name": "pangea-models",
  "version": "0.0.1",
  "description": "front and back end models with json schema",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@ctek/typed-utils": "^1.1.3",
    "@types/chai": "^3.5.1",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.51",
    "@types/ramda": "^0.24.9",
    "@types/sinon": "^1.16.35",
    "cpy-cli": "^1.0.1",
    "moment": "^2.18.1",
    "ramda": "^0.24.1",
    "typedoc": "^0.8.0",
    "typescript": "2.3.4",
    "typings": "2.1.1"
  },
  "scripts": {
    "prepare:dev": "npm run clean:dev && npm run lint-ts && npm run tsc:dev",
    "prepare:dist": "npm run clean:dist && npm run lint-ts && npm run tsc:dist",
    "prepare:test": "npm run clean:test && npm run lint-ts && npm run tsc:test",
    "predocs": "rm -rf docs",
    "docs": "typedoc --tsconfig tsconfig.main.json --excludePrivate --mode file --name \"Planner\" --out docs",
    "tsc:dev": "tsc -p tsconfig.main.json",
    "tsc:dist": "tsc -p tsconfig.main.json --outDir ./dist",
    "tsc:test": "tsc -p tsconfig.test.json",
    "clean:dev": "rm index.html index.js",
    "clean:dist": "rm -rf dist",
    "clean:test": "rm -rf test",
    "lint-ts": "tslint -c ./tslint.json './src/**/*.ts' --fix",
    "test": "npm run prepare:dev && npm run prepare:test",
    "install:typings": "rm -rf typings && typings i"
  },
  "author": "Century Tech",
  "dependencies": {
    "@types/z-schema": "^3.16.31",
    "z-schema": "^3.19.0"
  }
}
